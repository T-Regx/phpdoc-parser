{
  "name": "jasny/phpdoc-parser",
  "description": "Jasny skeleton library",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Arnold Daniels",
      "email": "arnold@jasny.net",
      "homepage": "http://www.jasny.net"
    }
  ],
  "require": {
    "php": "~8.0.0 | >=7.1.0",
    "jasny/php-functions": "3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.4 || ^8.0 || ^7.5"
  },
  "autoload": {
    "psr-4": {
      "Jasny\\PhpdocParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "test/",
      "Test\\ReadMe\\": "test/ReadMe/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
